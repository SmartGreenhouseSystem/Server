            <path d="M6 18l0 -3"></path>
            <path d="M10 18l0 .01"></path>
            <path d="M14 18l0 .01"></path>
            <path d="M18 18l0 .01"></path>
        </svg>
    <% when 2 %>
        <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-antenna-bars-3" width="24" height="24" viewBox="0 0 24 24" stroke-width="1" stroke="currentColor" fill="none" stroke-linecap="round" stroke-linejoin="round">
            <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
