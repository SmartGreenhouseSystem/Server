        <path d="M6 18v-3"></path>
        <path d="M10 18v-6"></path>
        <path d="M14 18v-4"></path>
        <path d="M14 10v-1"></path>
        <path d="M18 14v-8"></path>
        <path d="M3 3l18 18"></path>
    </svg>
<% else %>
    <% strength = 2 * (device.rssi + 100) %>
